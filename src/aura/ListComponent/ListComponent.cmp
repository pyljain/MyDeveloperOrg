<aura:component >
    <aura:handler name="init" value="{!this}" action="{! c.initialize }" />
    <aura:attribute name="shoppingList" type="String[]" />
    
    <div>
        <ul>
            <aura:iteration items="{! v.shoppingList }" var="item">
                <li>{! item }</li>
            </aura:iteration>
        </ul>
    </div>
</aura:component>