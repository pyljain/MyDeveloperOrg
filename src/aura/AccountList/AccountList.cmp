<aura:component controller="MyAccountService">
    <aura:handler name="init" value="{! this }" action="{! c.init }" />
    
    <div>
        <ul>
        	<aura:iteration items="{! v.accounts }" var="acc">
                <li>{! acc.Name }</li>
            </aura:iteration>
        </ul>
    </div>
</aura:component>